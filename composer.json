{
  "name": "moodle/site",
  "description": "Project Template to create a new Moodle Site",
  "type": "project",
  "require": {
    "moodle/composer-installer": "dev-main",
    "moodle/core-composer-scaffold": "dev-main",
    "moodle/moodle": "dev-composed"
  },
  "extra": {
    "install-path": "moodle/"
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.mymoodle.dev"
    }
  ],
  "config": {
    "allow-plugins": {
      "moodle/composer-installer": true,
      "cweagans/composer-patches": true,
      "moodle/core-composer-scaffold": true
    }
  }
}
